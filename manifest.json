{
  "manifest_version": 3,
  "name": "Tab Groups & Windows List",
  "version": "1.2.54",
  "description": "A 3-level expandable hierarchy of Windows, Tab Groups, and Tabs.",
  "permissions": [
    "tabGroups",
    "windows",
    "nativeMessaging",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
