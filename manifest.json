{
  "manifest_version": 3,
  "name": "Tab Groups & Windows List",
  "version": "1.1.0",
  "description": "A simple 2-level expandable list of current tab groups and their window names.",
  "permissions": [
    "tabGroups",
    "windows"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
